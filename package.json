{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate-api && next dev",
    "build": "npm run generate-api && next build",
    "start": "npm run generate-api && next start",
    "lint": "npm run generate-api && next lint",
    "generate-api": "openapi -i src/api/openapi.yml -o src/api/generated"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "localstorage-slim": "^2.2.0",
    "next": "12.1.6",
    "openapi-typescript": "^5.3.0",
    "openapi-typescript-codegen": "^0.22.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "17.0.34",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.4",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.15.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
