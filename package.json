    "lint": "npm run generate-api && next lint",
    "generate-api": "openapi -i src/api/openapi.yml -o src/api/generated"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "localstorage-slim": "^2.3.0",
    "next": "^12.2.2",
    "openapi-typescript": "^5.4.1",
    "openapi-typescript-codegen": "^0.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.2",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4"
  }
